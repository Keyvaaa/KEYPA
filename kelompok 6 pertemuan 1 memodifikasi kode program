#include <iostream>
using namespace std;

int main() {
  int i, n, t;
cout << "Masukan batas awal : ";
cin >> i;
cout << "masukan batas akhir : ";
cin >> n;

cout << endl;

for (i; i>=n; i--) {
     if (i%2 == 1) {
        cout << i << " " ;
        t += i;
	  }
}

cout << "\njumlah : " << t << endl;
return 0;
}

